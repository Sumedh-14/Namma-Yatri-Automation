public class AndroidBackButton extends BaseClass {

    public void tapBack() throws InterruptedException {
        for(int i = 0; i < 3; i++) { 
        	// Example: Press back button three times
        	Thread.sleep(3000); 
        	driver.navigate().back();
        	System.out.println("Tapped "+(i+1)+"time");
           // Adjust as needed
           
        }
    }
}






