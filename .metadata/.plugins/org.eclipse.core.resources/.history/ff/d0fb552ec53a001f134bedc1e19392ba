package User.Android;

import base.BaseClass;
import io.appium.java_client.AppiumBy;

public class UserRatingScreen extends BaseClass{
	public void ratingScreen() {
		for(int i = 0;i<5;i++)
		{
		driver1.findElement(AppiumBy.xpath("//android.widget.ImageView[@content-desc="+(i+1)+"star : Selected \"]"));
		}
		Thread.sleep(2000);
		driver1.findElement(AppiumBy.xpath("//android.widget.TextView[@content-desc=\"Proceed Button with : No option selected\"]"));
		
	}

}
